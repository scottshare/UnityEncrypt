<?xml version="1.0" encoding="UTF-8" ?>
<ldml>
	<numbers>
		<currencyFormats>
			<currencyFormatLength >
				<currencyFormat >
					<pattern>#,##0.00;(#,##0.00)</pattern>
				</currencyFormat>
			</currencyFormatLength >
		</currencyFormats>
	</numbers>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="short">
						<dateFormat >
							<pattern>d/M/yyyy</pattern>
						</dateFormat>
					</dateFormatLength >
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="long">
						<timeFormat >
							<pattern>HH:mm:ss</pattern>
						</timeFormat>
					</timeFormatLength >
				</timeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>