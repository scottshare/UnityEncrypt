<?xml version="1.0" encoding="UTF-8" ?>
<ldml>
	<numbers>
		<percentFormats>
			<percentFormatLength >
				<percentFormat >
					<pattern>#,##0 %</pattern>
				</percentFormat>
			</percentFormatLength >
		</percentFormats>
	</numbers>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="short">
						<dateFormat >
							<pattern>dd-MM-yyyy</pattern>
						</dateFormat>
					</dateFormatLength >
                                        <dateFormatLength type="month_day">
                                                <dateFormat>
                                                        <pattern>dd. MMMM</pattern>
                                                </dateFormat>
                                        </dateFormatLength>
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="long">
						<timeFormat >
							<pattern>HH:mm:ss</pattern>
						</timeFormat>
					</timeFormatLength >
				</timeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>
