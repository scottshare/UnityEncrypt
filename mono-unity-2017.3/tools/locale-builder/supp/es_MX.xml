<?xml version="1.0" encoding="UTF-8" ?>
<ldml>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="short">
						<dateFormat >
							<pattern>dd/MM/yyyy</pattern>
						</dateFormat>
					</dateFormatLength >
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="long">
						<timeFormat >
							<pattern>hh:mm:ss tt</pattern>
						</timeFormat>
					</timeFormatLength >
				</timeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>