<?xml version="1.0" encoding="UTF-8" ?>
<ldml>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<am>AM</am>
				<pm>PM</pm>
				<dateFormats>
					<dateFormatLength type="short">
						<dateFormat >
							<pattern>dd/MM/yyyy</pattern>
						</dateFormat>
						<extraPatterns>
						  <pattern>yyyy-MM-dd</pattern>
						</extraPatterns>
					</dateFormatLength >
					<dateFormatLength type="long">
					  <dateFormat>
					    <pattern>dddd, dd MMMM yyyy</pattern>
					  </dateFormat>
					</dateFormatLength>
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="long">
						<timeFormat >
							<pattern>hh:mm:ss tt</pattern>
						</timeFormat>
						<extraPatterns>
						  <pattern>HH:mm:ss</pattern>
						</extraPatterns>
					</timeFormatLength >
				</timeFormats>
			</calendar>
		</calendars>
	</dates>
  <numbers>
    <decimalFormats>
      <decimalFormatLength >
        <decimalFormat >
          <pattern>#,##0.###;-#,##0.###</pattern>
        </decimalFormat>
      </decimalFormatLength >
    </decimalFormats>
  </numbers>
</ldml>
